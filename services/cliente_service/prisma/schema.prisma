generator client {
  provider = "prisma-client-js"
  //output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cliente{
  id Int @id @default(autoincrement())
  nombre String? // Con el signo de interrogacion ahora es opcional
  email String? @unique
  telefono String?
  createdAt DateTime @default(now())
}